<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="edu.uw.tcss450.angelans.lab1myfirstapp.ui.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="edu.uw.tcss450.angelans.lab1myfirstapp.ui.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" >
        <argument
            android:name="message"
            app:argType="string" />
        <action
            android:id="@+id/action_secondFragment_to_angelaFragment"
            app:destination="@id/angelaFragment" />
    </fragment>
    <fragment
        android:id="@+id/angelaFragment"
        android:name="edu.uw.tcss450.angelans.lab1myfirstapp.ui.AngelaFragment"
        android:label="fragment_angela"
        tools:layout="@layout/fragment_angela" />
</navigation>